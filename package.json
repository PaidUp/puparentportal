  "scripts": {
    "dev": "export APP_ENV=local && webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "start": "npm run dev",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "e2e": "node test/e2e/runner.js",
    "test1": "npm run unit && npm run e2e",
    "test": "ls",
    "lint": "eslint --ext .js,.vue src test/unit/specs test/e2e/specs",
    "build": "node build/build.js",
    "build-theme": "sass --style expanded build/theme-input.scss src/style/theme.css",
    "build-dev": "export APP_ENV=develop && cp src/config/development.js src/config/index.js && npm run build",
    "build-stg": "export APP_ENV=develop && cp src/config/stage.js src/config/index.js && npm run build",
    "build-prod": "export APP_ENV=production && cp src/config/production.js src/config/index.js && npm run build"
