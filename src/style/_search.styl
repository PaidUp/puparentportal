.search-page
  .not-found
    margin: 50px auto 0 auto
    text-align: center
    width 100%
    font-size: 14px
    color: $cgray1
    line-height: 1.3
    .md-icon
      color: $ca1
      margin-bottom: 15px
  .table-title
    color: $cgray2
    margin-bottom: 5px
    margin-top: 30px
  .column-25
    width 25%
  .column-with-img
    .md-table-cell-container
      display: flex
      flex-flow: row wrap
      justify-content: flex-start
      align-items: center
      .md-avatar
      .md-icon
      img.club-img
        margin: 0 8px 0 0
      img.club-img
        width 25px
