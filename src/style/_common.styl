.md-app
  height 100vh
.md-drawer
  width: 255px
.md-button-content
  text-transform: none
a, a:hover, a:visited, a:link, a:active, a > *
  text-decoration: none !important

mixin-placeholder(c)
  &::-webkit-input-placeholder
    color: c;
  &::-moz-placeholder
    color: c;
  &:-ms-input-placeholder
    color: c;
  &:-moz-placeholder
    color: c;